<progress percent="{{percent}}" stroke-width="5" />
<view class="directory">
  <view wx:for="{{chapters}}" wx:key="*" wx:for-item="chapter" class="directory-item">
    <navigator url="../show/show?id={{id}}&path={{chapter.path}}" >
      <text>{{chapter.level}} </text>
      <text>{{chapter.title}}</text>
    </navigator>
  </view>
</view>
